<nav class="UnderlineNav UnderlineNav--full" aria-label="Menu">

 <div class="container-lg UnderlineNav-container">
   <div class="UnderlineNav-body">
     <%= image_tag "ce-cert-logo.png" %>
     <%= link_to "Home", home(current_user), :class => home_page?(home(current_user)) %>
     <% if is_superadmin? %>
      <%= link_to "Instructors", instructors_path, :class => current_class?(instructors_path)  %>
     <% end %>
     <% unless is_attendee? %>
      <%= link_to "Courses", courses_path, :class => current_class?(courses_path) %>
      <%= link_to "Attendees", attendees_path, :class => current_class?(attendees_path)  %>
      <%= link_to "Evaluations", evaluations_path, :class => current_class?(evaluations_path)  %>
      <%= link_to "Questions", questions_path, :class => current_class?(questions_path)  %>
     <% end %>
     <% if is_superadmin? %>
      <%= link_to "Users", users_path, :class => current_class?(users_path)  %>
      <%= link_to "Finished Evals", finished_evaluations_path, :class => current_class?(finished_evaluations_path)  %>
     <% end %>
    </div>
     <div class="float-right UnderlineNav-body">
       <% unless !user_assigned?(current_user) %>
        <%= link_to profile(current_user), class: "UnderlineNav-item" do %>
        <%= octicon "person" %>
        Profile
        <% end %>
       <% end %>
       <%= link_to destroy_user_session_path, method: :delete, class: "UnderlineNav-item" do %>
        <%= octicon "sign-out" %>
        Logout
       <% end %>
     </div>
   </div>
</nav>
