<%= render 'evaluations_subnav' %>

 <ul>
    <% @evaluations.each do |e| %>
    <% if is_authorized?(e) %>
      <li class="col-12 d-block width-full py-4 border-bottom public source">
          <div class="d-inline-block mb-1">
            <h3>
              <%= link_to e.name, evaluation_path(e), itemprop: "name codeRepository" %>
            </h3>
          </div>
          <div class="f6 text-gray mt-2">
                <span class="mr-3" itemprop="programmingLanguage">
                  Class: <%= e.course.title unless e.course.nil? %>
                </span>
              Date: <%= e.course.start_date unless e.course.nil? %>
          </div>
        </li>
      <% end %>
    <% end %>
  </ul>
